{% extends 'base.html.twig' %}

{% block title %}Revenu{% endblock %}

{% block body %}
    <h1>Revenu</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ revenu.id }}</td>
            </tr>
            <tr>
                <th>Titre</th>
                <td>{{ revenu.titre }}</td>
            </tr>
            <tr>
                <th>Date</th>
                <td>{{ revenu.date ? revenu.date|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Montant</th>
                <td>{{ revenu.montant }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_revenu_index') }}">back to list</a>

    <a href="{{ path('app_revenu_edit', {'id': revenu.id}) }}">edit</a>

    {{ include('revenu/_delete_form.html.twig') }}
{% endblock %}
